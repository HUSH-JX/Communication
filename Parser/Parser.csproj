<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags></PackageTags>
    <Description>数据分帧解析器</Description>
    <Version>5.0.5</Version>
    <Authors>CSoft</Authors>
    <PackageId>CSoft.Parser</PackageId>
    <PackageReleaseNotes>数据分帧解析器</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/caoyiliang/Communication</PackageProjectUrl>
    <RepositoryUrl>https://github.com/caoyiliang/Communication</RepositoryUrl>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Parser.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CSoft.LogInterface" Version="5.0.2" />
    <PackageReference Include="CSoft.Utils" Version="5.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
